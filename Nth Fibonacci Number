public static void main(String[] args) {
		
		
		Scanner sc=new Scanner(System.in);
	    int n=sc.nextInt();
		int n1=1,n2=1,sum=0; 
		if(n!=1&& n!=2)
    {
        for(int i=2;i<n;++i)//loop starts from 2 because 0 and 1 are already printed    
        {    
        sum=n1+n2;    
         
        n1=n2;    
        n2=sum;    
     }  
     }
     else
     {
		 sum=1;
	 }  
       System.out.print(sum);

		
	}
