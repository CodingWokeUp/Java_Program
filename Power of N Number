public static void main(String[] args) {
        // Write your code here
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        int p=sc.nextInt();
        int result=1;
        if(n!=0){
            for(int i=1;i<=p;i++){
                result=result*n;
            }
            System.out.print(result);
        }else if(n==0&& p==0){
            System.out.print(1);
        }
        else{
            System.out.print(0);
        }
        
    }
