public static void main(String[] args) 
{
		
		
		Scanner sc=new Scanner(System.in);
		int n = sc.nextInt();
	    int p=0;
		for(int i=1;i<=n;i++)
    {
			int temp=0;
			for(int j=1;j<=i;j++)
      {
				 char ch=(char)('A'+temp+p);
					 
					 System.out.print(ch);
					 temp=temp+1;
				 }
				 p++;
				 System.out.print("\n");
			 }

		
	}
