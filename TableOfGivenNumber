/////////////////////////////////////////////////////// java code write the table given number ////////////////////////////////
import java.util.*;

public class Main
{
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		for(int i= 1; i<=10; i++){
		    int multi = i*n;
		    System.out.println( n +" * "+ i +" = "+ multi);
		    
		}
	}
}
