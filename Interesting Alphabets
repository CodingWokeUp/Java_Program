public static void main(String[] args) 
{
		//Your code goes here
		 Scanner sc = new Scanner(System.in);
			int n = sc.nextInt();
			int p=n;
			 for(int i=0;i<=n;i++)
       {
				 int temp=0;
				 for(int j=1;j<=i;j++)
         {
				 char ch=(char)('A'+(p-1)+j);
					  System.out.print(ch);
					 
				 }
				 temp=temp+1;
				 p--;
				 System.out.print("\n");
			 }
	}
