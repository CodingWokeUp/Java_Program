//code one
 public static int secondLargestElement(int[] arr) {
    	//Your code goes here
        int large=Integer. MIN_VALUE ,temp=Integer. MIN_VALUE  ;
        for(int i=0;i<arr.length;i++){
              if(arr[i]>large){
                  temp=large;
                  large=arr[i];
              }
        }
        return temp;
    }
