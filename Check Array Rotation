 public static int arrayRotateCheck(int[] arr){
    	//Your code goes here
        int i=0,temp=0;
        while(i<arr.length){
            if(arr.length==0){
                temp=0;
            }
            else if(arr[i]<arr[i+1]){
                 i++;
            }else{
                temp=1+i;
                break;
            }
        }
        return temp;
    }
