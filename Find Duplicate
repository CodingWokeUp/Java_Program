// just logic
//function
 public static int duplicateNumber(int arr[]) {
    	//Your code goes here
        int p=0,x=0,n=arr.length;
        for(int i=0;i<n;i++){
            for(int j=i+1;j<n;j++){
                if(arr[i]==arr[j]){
                    x=arr[j];
                }
            }
        }
        return x;
    }
