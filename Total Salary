public static void main(String[] args) {
		// Write your code here
       Scanner sc=new Scanner(System.in);
	   int bs=sc.nextInt();
	   String c=sc.next();
	   char grade=c.charAt(0);
	   int allow;
	   if(grade==65){
		   allow=1700;
	   }else if(grade==66){
		   allow=1500;
	   }else{
		   allow=1300;
	   }
       double totalSalary=((bs+(0.20*bs)+(0.50*bs)+allow)-(0.11*bs));
	   double ts=Math.round(totalSalary);
	   int t=(int)(ts);
	   System.out.print(t);
	}
