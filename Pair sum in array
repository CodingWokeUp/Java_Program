public static int pairSum(int[] arr, int num)
{
		//Your code goes here
		int sum=0,count=0;
		for(int i=0;i<arr.length;i++)
    {
         for(int j=i+1;j<arr.length;j++)
               {
				   sum=arr[i]+arr[j];
				   if(sum==num)
           {
					   count++;
				   }
			   }
		}
		return count;
	}
