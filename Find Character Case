public static void main(String[] args) {
        // Write your code here
        Scanner sc=new Scanner(System.in);
        String ch=sc.next();
        char c=ch.charAt(0);
        if(65<=c&&c<=90){
            System.out.print(1);
        }else  if(97<=c&&c<=122){
            System.out.print(0);
        }else{
             System.out.print(-1);
        }

    }
