//just logic
// function 
public static int findUnique(int[] arr){
		//Your code goes here
    int n=arr.length,j=1,i=0,x=0,p=0;
    for (i=0;i<n;i++){
      for(j=i+1;j<n;j++){
        if(arr[i]==arr[j]){
          p++;
          break;
        } else{
          p=0;
        }
         if(p==0){
          x=i;
       }
      }
     
      }       
   return x;
 }
